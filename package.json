{
  "name": "react-to-print",
  "version": "2.9.0",
  "description": "Print React components in the browser",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "webpack-dev-server --env.development --progress --colors --hot --open",
    "build": "webpack --env.production --progress",
    "start:electron": "cross-env NODE_ENV=development electron example/ElectronPrint/electron/electron.js",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregnb/react-to-print.git"
  },
  "keywords": [
    "react",
    "print",
    "reactjs",
    "react-to-print"
  ],
  "author": "gregnb <gregnb@gmail.com>",
  "contributors": [
    "Matthew Herbst"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gregnb/react-to-print/issues"
  },
  "homepage": "https://github.com/gregnb/react-to-print#readme",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "acorn": "^7.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "electron": "^9.1.1",
    "eslint": "^7.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "cross-env": "^7.0.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  }
}
